{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "globals": {},
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["react", "@typescript-eslint","prettier"],
  "extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended","prettier"],
  "rules": {
        "prettier/prettier": ["error"]
  }
}
